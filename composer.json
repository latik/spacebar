{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "enqueue/amqp-bunny": "^0.9.12",
        "friendsofsymfony/elastica-bundle": "^5.1",
        "knplabs/knp-markdown-bundle": "^1.7",
        "knplabs/knp-paginator-bundle": "^2.8@dev",
        "knplabs/knp-time-bundle": "^1.1@dev",
        "nexylan/slack-bundle": "^2.1",
        "nyholm/psr7": "^1.2",
        "sensio/framework-extra-bundle": "^5.2",
        "sroze/messenger-enqueue-transport": "^0.3.1",
        "stof/doctrine-extensions-bundle": "^1.3@dev",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.2",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer-pack": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "twig/extensions": "^1.5@dev"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0@dev",
        "infection/infection": "^0.13.4",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.5",
        "phpstan/phpstan": "^0.11.8",
        "rector/rector": "^0.5.5",
        "sensiolabs/security-checker": "^6.0",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "*",
        "symplify/coding-standard": "^6.0",
        "symplify/easy-coding-standard": "^6.0",
        "thecodingmachine/phpstan-strict-rules": "^0.11.2",
        "vimeo/psalm": "^3.6"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpstan": "phpstan analyse -c phpstan.neon --level max src tests",
        "psalm": "psalm",
        "cs-check": "ecs check src",
        "rector": "rector process src -c rector.yaml"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4"
        }
    }
}
